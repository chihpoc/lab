@prefix : <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix vin: <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix food: <http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine> .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine> rdf:type owl:Ontology ;
                                                        owl:imports <http://www.w3.org/TR/2003/PR-owl-guide-20031209/food> ;
                                                        rdfs:comment "An example OWL ontology" ,
                                                                     """Derived from the DAML Wine ontology at 
      http://ontolingua.stanford.edu/doc/chimaera/ontologies/wines.daml
      Substantially changed, in particular the Region based relations.
    """ ;
                                                        rdfs:label "Wine Ontology" ;
                                                        owl:priorVersion <http://www.w3.org/TR/2003/CR-owl-guide-20030818/wine> .

#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#adjacentRegion
vin:adjacentRegion rdf:type owl:ObjectProperty ,
                            owl:SymmetricProperty ;
                   rdfs:domain vin:Region ;
                   rdfs:range vin:Region .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasBody
vin:hasBody rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf vin:hasWineDescriptor ;
            rdf:type owl:FunctionalProperty ;
            rdfs:range vin:WineBody .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasColor
vin:hasColor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf vin:hasWineDescriptor ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain vin:Wine ;
             rdfs:range vin:WineColor .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasFlavor
vin:hasFlavor rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf vin:hasWineDescriptor ;
              rdf:type owl:FunctionalProperty ;
              rdfs:range vin:WineFlavor .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasMaker
vin:hasMaker rdf:type owl:ObjectProperty ;
             owl:inverseOf vin:producesWine ;
             rdf:type owl:FunctionalProperty .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasSugar
vin:hasSugar rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf vin:hasWineDescriptor ;
             rdf:type owl:FunctionalProperty ;
             rdfs:range vin:WineSugar .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasVintageYear
vin:hasVintageYear rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain vin:Vintage ;
                   rdfs:range vin:VintageYear .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasWineDescriptor
vin:hasWineDescriptor rdf:type owl:ObjectProperty ;
                      rdfs:domain vin:Wine ;
                      rdfs:range vin:WineDescriptor .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#locatedIn
vin:locatedIn rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain owl:Thing ;
              rdfs:range vin:Region .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#madeFromGrape
vin:madeFromGrape rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf food:madeFromFruit ;
                  owl:inverseOf vin:madeIntoWine ;
                  rdfs:domain vin:Wine ;
                  rdfs:range vin:WineGrape .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#madeIntoWine
vin:madeIntoWine rdf:type owl:ObjectProperty .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#producesWine
vin:producesWine rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#yearValue
vin:yearValue rdf:type owl:DatatypeProperty ;
              rdfs:domain vin:VintageYear ;
              rdfs:range xsd:positiveInteger .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#AlsatianWine
vin:AlsatianWine rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty vin:locatedIn ;
                                                              owl:hasValue vin:AlsaceRegion
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#AmericanWine
vin:AmericanWine rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty vin:locatedIn ;
                                                              owl:hasValue vin:USRegion
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Anjou
vin:Anjou rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( vin:Loire
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty vin:locatedIn ;
                                                       owl:hasValue vin:AnjouRegion
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty vin:hasBody ;
                            owl:hasValue vin:Light
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty vin:hasColor ;
                            owl:hasValue vin:Rose
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty vin:hasFlavor ;
                            owl:hasValue vin:Delicate
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty vin:hasSugar ;
                            owl:hasValue vin:OffDry
                          ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Beaujolais
vin:Beaujolais rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty vin:locatedIn ;
                                                            owl:hasValue vin:BeaujolaisRegion
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty vin:hasBody ;
                                 owl:hasValue vin:Light
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty vin:hasColor ;
                                 owl:hasValue vin:Red
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty vin:hasFlavor ;
                                 owl:hasValue vin:Delicate
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty vin:hasSugar ;
                                 owl:hasValue vin:Dry
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty vin:madeFromGrape ;
                                 owl:hasValue vin:GamayGrape
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty vin:madeFromGrape ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger
                               ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Bordeaux
vin:Bordeaux rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty vin:locatedIn ;
                                                          owl:hasValue vin:BordeauxRegion
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Burgundy
vin:Burgundy rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty vin:locatedIn ;
                                                          owl:hasValue vin:BourgogneRegion
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty vin:hasSugar ;
                               owl:hasValue vin:Dry
                             ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CabernetFranc
vin:CabernetFranc rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty vin:madeFromGrape ;
                                                               owl:hasValue vin:CabernetFrancGrape
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty vin:madeFromGrape ;
                                                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty vin:hasBody ;
                                    owl:hasValue vin:Medium
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty vin:hasColor ;
                                    owl:hasValue vin:Red
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty vin:hasFlavor ;
                                    owl:hasValue vin:Moderate
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty vin:hasSugar ;
                                    owl:hasValue vin:Dry
                                  ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CabernetSauvignon
vin:CabernetSauvignon rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty vin:madeFromGrape ;
                                                                   owl:hasValue vin:CabernetSauvignonGrape
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty vin:madeFromGrape ;
                                                                   owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty vin:hasBody ;
                                        owl:allValuesFrom [ rdf:type owl:Class ;
                                                            owl:oneOf ( vin:Full
                                                                        vin:Medium
                                                                      )
                                                          ]
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty vin:hasFlavor ;
                                        owl:allValuesFrom [ rdf:type owl:Class ;
                                                            owl:oneOf ( vin:Moderate
                                                                        vin:Strong
                                                                      )
                                                          ]
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty vin:hasColor ;
                                        owl:hasValue vin:Red
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty vin:hasSugar ;
                                        owl:hasValue vin:Dry
                                      ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CaliforniaWine
vin:CaliforniaWine rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty vin:locatedIn ;
                                                                owl:hasValue vin:CaliforniaRegion
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Chardonnay
vin:Chardonnay rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty vin:madeFromGrape ;
                                                            owl:hasValue vin:ChardonnayGrape
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty vin:madeFromGrape ;
                                                            owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty vin:hasBody ;
                                 owl:allValuesFrom [ rdf:type owl:Class ;
                                                     owl:oneOf ( vin:Full
                                                                 vin:Medium
                                                               )
                                                   ]
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty vin:hasFlavor ;
                                 owl:allValuesFrom [ rdf:type owl:Class ;
                                                     owl:oneOf ( vin:Moderate
                                                                 vin:Strong
                                                               )
                                                   ]
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty vin:hasColor ;
                                 owl:hasValue vin:White
                               ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CheninBlanc
vin:CheninBlanc rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty vin:madeFromGrape ;
                                                             owl:hasValue vin:CheninBlancGrape
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty vin:madeFromGrape ;
                                                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasBody ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:oneOf ( vin:Full
                                                                  vin:Medium
                                                                )
                                                    ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasSugar ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:oneOf ( vin:Dry
                                                                  vin:OffDry
                                                                )
                                                    ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasColor ;
                                  owl:hasValue vin:White
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasFlavor ;
                                  owl:hasValue vin:Moderate
                                ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Chianti
vin:Chianti rdf:type owl:Class ;
            rdfs:subClassOf vin:ItalianWine ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty vin:hasBody ;
                              owl:allValuesFrom [ rdf:type owl:Class ;
                                                  owl:oneOf ( vin:Light
                                                              vin:Medium
                                                            )
                                                ]
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty vin:hasColor ;
                              owl:hasValue vin:Red
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty vin:hasFlavor ;
                              owl:hasValue vin:Moderate
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty vin:hasSugar ;
                              owl:hasValue vin:Dry
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty vin:locatedIn ;
                              owl:hasValue vin:ChiantiRegion
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty vin:madeFromGrape ;
                              owl:hasValue vin:SangioveseGrape
                            ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CotesDOr
vin:CotesDOr rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( vin:RedBurgundy
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty vin:locatedIn ;
                                                          owl:hasValue vin:CotesDOrRegion
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty vin:hasFlavor ;
                               owl:hasValue vin:Moderate
                             ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DessertWine
vin:DessertWine rdf:type owl:Class ;
                rdfs:subClassOf vin:Wine ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasSugar ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:oneOf ( vin:OffDry
                                                                  vin:Sweet
                                                                )
                                                    ]
                                ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DryRedWine
vin:DryRedWine rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( vin:DryWine
                                                          vin:RedWine
                                                        ) ;
                                     rdf:type owl:Class
                                   ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DryRiesling
vin:DryRiesling rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( vin:Riesling
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty vin:hasSugar ;
                                                             owl:hasValue vin:Dry
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasBody ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:oneOf ( vin:Light
                                                                  vin:Medium
                                                                )
                                                    ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasColor ;
                                  owl:hasValue vin:White
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasFlavor ;
                                  owl:hasValue vin:Delicate
                                ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DryWhiteWine
vin:DryWhiteWine rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( vin:DryWine
                                                            vin:WhiteWine
                                                          ) ;
                                       rdf:type owl:Class
                                     ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DryWine
vin:DryWine rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty vin:hasSugar ;
                                                         owl:hasValue vin:Dry
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#EarlyHarvest
vin:EarlyHarvest rdf:type owl:Class ;
                 rdfs:subClassOf vin:Wine ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty vin:hasSugar ;
                                   owl:allValuesFrom [ rdf:type owl:Class ;
                                                       owl:oneOf ( vin:Dry
                                                                   vin:OffDry
                                                                 )
                                                     ]
                                 ] ;
                 owl:disjointWith vin:LateHarvest .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#FrenchWine
vin:FrenchWine rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty vin:locatedIn ;
                                                            owl:hasValue vin:FrenchRegion
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#FullBodiedWine
vin:FullBodiedWine rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty vin:hasBody ;
                                                                owl:hasValue vin:Full
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Gamay
vin:Gamay rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty vin:madeFromGrape ;
                                                       owl:hasValue vin:GamayGrape
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty vin:madeFromGrape ;
                                                       owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#GermanWine
vin:GermanWine rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty vin:locatedIn ;
                                                            owl:hasValue vin:GermanyRegion
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#IceWine
vin:IceWine rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( vin:DessertWine
                                                       vin:LateHarvest
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty vin:hasColor ;
                                                         owl:hasValue vin:White
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty vin:hasBody ;
                              owl:allValuesFrom [ rdf:type owl:Class ;
                                                  owl:oneOf ( vin:Full
                                                              vin:Medium
                                                            )
                                                ]
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty vin:hasFlavor ;
                              owl:allValuesFrom [ rdf:type owl:Class ;
                                                  owl:oneOf ( vin:Moderate
                                                              vin:Strong
                                                            )
                                                ]
                            ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#ItalianWine
vin:ItalianWine rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty vin:locatedIn ;
                                                             owl:hasValue vin:ItalianRegion
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#LateHarvest
vin:LateHarvest rdf:type owl:Class ;
                rdfs:subClassOf vin:Wine ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasFlavor ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:oneOf ( vin:Moderate
                                                                  vin:Strong
                                                                )
                                                    ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vin:hasSugar ;
                                  owl:hasValue vin:Sweet
                                ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Loire
vin:Loire rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( vin:Wine
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty vin:locatedIn ;
                                                       owl:hasValue vin:LoireRegion
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Margaux
vin:Margaux rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( vin:Medoc
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty vin:locatedIn ;
                                                         owl:hasValue vin:MargauxRegion
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty vin:hasFlavor ;
                              owl:hasValue vin:Delicate
                            ] ,
