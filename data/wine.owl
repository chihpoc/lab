vin:WineDescriptor rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:unionOf ( vin:WineColor
                                                       vin:WineTaste
                                                     )
                                       ] ;
                   rdfs:comment "Made WineDescriptor unionType of tastes and color" .


